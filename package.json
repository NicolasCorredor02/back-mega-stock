{
  "name": "api-mega-stock",
  "version": "1.0.0",
  "description": "Project backend for Mega Stock Ecommerce",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node --watch ./src/index.js",
    "setup-alias": "link-module-alias",


    "db:mysql:migrate": "dotenv -e .env -- npx prisma migrate dev --schema=./prisma/prisma-mysql/schema.prisma",
    "db:mysql:push": "dotenv -e .env -- npx prisma db push --schema=./prisma/prisma-mysql/schema.prisma",
    "db:mysql:studio": "dotenv -e .env -- npx prisma studio --schema=./prisma/prisma-mysql/schema.prisma",
    "db:mysql:generate": "dotenv -e .env -- npx prisma generate --schema=./prisma/prisma-mysql/schema.prisma",
    
    "db:mongodb:push": "dotenv -e .env -- npx prisma db push --schema=./prisma/prisma-mongodb/schema.prisma",
    "db:mongodb:studio": "dotenv -e .env -- npx prisma studio --schema=./prisma/prisma-mongodb/schema.prisma",
    "db:mongodb:generate": "dotenv -e .env -- npx prisma generate --schema=./prisma/prisma-mongodb/schema.prisma"
  },
  "_moduleAliases": {
    "root": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasCorredor02/back-mega-stock.git"
  },
  "keywords": [
    "backend",
    "ExpressJs",
    "NodeJS",
    "JavaScript",
    "Ecommerce",
    "APIs",
    "Restful"
  ],
  "author": "NicolasCorredor02",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NicolasCorredor02/back-mega-stock/issues"
  },
  "homepage": "https://github.com/NicolasCorredor02/back-mega-stock#readme",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "1.41.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "express-handlebars": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "link-module-alias": "^1.2.0",
    "mongoose": "^8.11.0",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "prisma": "^6.7.0",
    "standard": "^17.1.2"
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
